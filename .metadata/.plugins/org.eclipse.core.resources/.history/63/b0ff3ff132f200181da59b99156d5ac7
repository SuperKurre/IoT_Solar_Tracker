#ifndef RAPIDATA_H_
#define RAPIDATA_H_

class RapiData {
public:
	RapiData(double q=0, double p=0, rotA=0, tiltA=0) : chargeAs(q), powerW(p), rotationAngle(rotA), tiltAngle(tiltA) {}
	virtual ~RapiData();


private:
	double chargeAs;	//baattery charge level
	double powerW;	//instantaneous power reading
	int rotationAngle; //deg
	int tiltAngle; //deg
};

#endif /* RAPIDATA_H_ */
